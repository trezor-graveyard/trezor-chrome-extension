{
  "name": "TREZOR Chrome Extension",
  "description": "Chrome extension for TREZOR",
  "version": "1.2.0",
  "author": "SatoshiLabs",
  "short_name": "TREZOR Extension",

  "manifest_version": 2,
  "minimum_chrome_version": "38.0.2125.7",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },

  "sockets": {
    "udp": {
      "bind": ["127.0.0.1:21327", "127.0.0.1:21328","127.0.0.1:21329"],
      "send": ["127.0.0.1:21324", "127.0.0.1:21325","127.0.0.1:21326"]
    }
  },
  "app": {
    "background": {
      "scripts": ["index.js"]
    }
  },
  "externally_connectable": "WILL BE CHANGED BY SCRIPT",
  "permissions": [
    "hid", {
      "usbDevices": [{
        "vendorId": 21324,
        "productId": 1
      }]
    },
    "storage"
  ],
  "key": ""
}
